{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.SetReportPublish",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Set a Report's publish status</p></label><br/><p>Sets the publish status of the report and returns the new status, including the URL of any enabled publishing.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "readOnlyFullAccessibleBy": {
                        "description": "Indicates who can access the 'Read-Only Full' view of the published report:\n* If **ALL**, it is available to anyone who has the link.\n* If **ORG**, it is available only to members of the report owner's Smartsheet organization account.\n* If **SHARED**, it is available only to users shared to the item.\n\nOnly returned in a response if **readOnlyFullEnabled** = **true**.\n",
                        "type": "string",
                        "path": "readOnlyFullAccessibleBy"
                    },
                    "readOnlyFullDefaultView": {
                        "description": "Indicates which view the user has set for a read-only, default view of the published report. Must be one of\nthe following values: **CALENDAR**, **CARD**, or **GRID**.\n",
                        "type": "string",
                        "path": "readOnlyFullDefaultView"
                    },
                    "readOnlyFullEnabled": {
                        "description": "(Required) If **true**, a rich version of the report is published with the ability to download row\nattachments and discussions.\n",
                        "type": "boolean",
                        "path": "readOnlyFullEnabled"
                    },
                    "readOnlyFullShowToolbar": {
                        "deprecated": true,
                        "description": "Indicates whether the left nav toolbar is displayed. The default, or **true**, is to display the toolbar.\nIf **false**, hides the toolbar.\n",
                        "type": "boolean",
                        "path": "readOnlyFullShowToolbar"
                    },
                    "readOnlyFullUrl": {
                        "description": "URL for 'Read-Only Full' view of the published report.\n\nOnly returned in a response if **readOnlyFullEnabled** = **true**.\n",
                        "type": "string",
                        "path": "readOnlyFullUrl"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "readOnlyFullAccessibleBy": {
                        "type": "text",
                        "index": 1,
                        "label": "Read Only Full Accessible By",
                        "tooltip": "<p>Indicates who can access the 'Read-Only Full' view of the published report:</p>\n<ul>\n<li>If <strong>ALL</strong>, it is available to anyone who has the link.</li>\n<li>If <strong>ORG</strong>, it is available only to members of the report owner's Smartsheet organization account.</li>\n<li>If <strong>SHARED</strong>, it is available only to users shared to the item.</li>\n</ul>\n<p>Only returned in a response if <strong>readOnlyFullEnabled</strong> = <strong>true</strong>.</p>"
                    },
                    "readOnlyFullDefaultView": {
                        "type": "text",
                        "index": 2,
                        "label": "Read Only Full Default View",
                        "tooltip": "<p>Indicates which view the user has set for a read-only, default view of the published report. Must be one of\nthe following values: <strong>CALENDAR</strong>, <strong>CARD</strong>, or <strong>GRID</strong>.</p>"
                    },
                    "readOnlyFullEnabled": {
                        "type": "toggle",
                        "index": 3,
                        "label": "Read Only Full Enabled",
                        "tooltip": "<p>(Required) If <strong>true</strong>, a rich version of the report is published with the ability to download row\nattachments and discussions.</p>"
                    },
                    "readOnlyFullShowToolbar": {
                        "type": "toggle",
                        "index": 4,
                        "label": "Read Only Full Show Toolbar",
                        "tooltip": "<p>Indicates whether the left nav toolbar is displayed. The default, or <strong>true</strong>, is to display the toolbar.\nIf <strong>false</strong>, hides the toolbar.</p>"
                    },
                    "readOnlyFullUrl": {
                        "type": "text",
                        "index": 5,
                        "label": "Read Only Full Url",
                        "tooltip": "<p>URL for 'Read-Only Full' view of the published report.</p>\n<p>Only returned in a response if <strong>readOnlyFullEnabled</strong> = <strong>true</strong>.</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Failed Items",
                    "value": "failedItems",
                    "schema": {
                        "description": "Array of BulkItemFailure objects which represents the items that failed to be added or updated. See [Partial Success](/partial-success) for more information. Applicable only for bulk operations that support partial success.",
                        "type": "array",
                        "nullable": true,
                        "items": {
                            "type": "object",
                            "properties": {
                                "rowId": {
                                    "description": "The Id of the row that failed. Applicable only to bulk row operations.",
                                    "type": "number",
                                    "nullable": true
                                },
                                "error": {
                                    "description": "The error caused by the failed item.",
                                    "type": "object",
                                    "properties": {
                                        "refId": {
                                            "description": "The Id of the specific error occurrence. Please include this information when contacting Smartsheet support.",
                                            "type": "string"
                                        },
                                        "errorCode": {
                                            "description": "Custom error code from Smartsheet. See the complete [Error Code List](../#section/Error-Code-List).",
                                            "type": "number"
                                        },
                                        "message": {
                                            "description": "Descriptive error message.",
                                            "type": "string"
                                        }
                                    }
                                },
                                "index": {
                                    "description": "The index of the failed item in the bulk request array.",
                                    "type": "number"
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Result",
                    "value": "result"
                },
                {
                    "label": "Result Read Only Full Accessible By",
                    "value": "result.readOnlyFullAccessibleBy"
                },
                {
                    "label": "Result Read Only Full Default View",
                    "value": "result.readOnlyFullDefaultView"
                },
                {
                    "label": "Result Read Only Full Enabled",
                    "value": "result.readOnlyFullEnabled"
                },
                {
                    "label": "Result Read Only Full Show Toolbar",
                    "value": "result.readOnlyFullShowToolbar"
                },
                {
                    "label": "Result Read Only Full Url",
                    "value": "result.readOnlyFullUrl"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}