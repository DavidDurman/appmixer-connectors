{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.sendReportViaEmail",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Send report via email</p></label><br/><p>Sends the report as a PDF attachment via email to the designated recipients</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "format": {
                        "description": "One of the following values: **EXCEL**, **PDF**, or **PDF_GANTT**.\n",
                        "type": "string",
                        "path": "format"
                    },
                    "ccMe": {
                        "description": "Indicates whether to send a copy of the email to the sender.",
                        "type": "boolean",
                        "path": "ccMe"
                    },
                    "message": {
                        "description": "The message of the email.",
                        "type": "string",
                        "path": "message"
                    },
                    "sendTo": {
                        "description": "Array of recipients.",
                        "type": "array",
                        "path": "sendTo",
                        "items": {
                            "description": "Specifies the recipient of an email. The recipient may be either an individual or a group. To specify an individual, set the email attribute; to specify a group, set the groupId attribute. Either email and groupId may be set, but not both.\n",
                            "oneOf": [
                                {
                                    "type": "object",
                                    "description": "Specifies the recipient of an [Email]($ref: 'Email.yaml#/components/schemas/Email'). The recipient may be either an individual or a group. To specify an individual, set the **email** attribute; to specify a group, set the **groupId** attribute. Either **email** and **groupId** may be set, but not both.\n",
                                    "properties": {
                                        "email": {
                                            "description": "The email address of an individual recipient.",
                                            "type": "string",
                                            "format": "email",
                                            "example": "john.doe@smartsheet.com"
                                        }
                                    }
                                },
                                {
                                    "type": "object",
                                    "description": "Specifies the recipient of an [Email]($ref: 'Email.yaml#/components/schemas/Email'). The recipient may be either an individual or a group. To specify an individual, set the **email** attribute; to specify a group, set the **groupId** attribute. Either **email** and **groupId** may be set, but not both.\n",
                                    "properties": {
                                        "groupId": {
                                            "description": "The Id of a group recipient.",
                                            "type": "number",
                                            "example": 2258118617917316
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "subject": {
                        "description": "The subject of the email.",
                        "type": "string",
                        "path": "subject"
                    },
                    "formatDetails|paperSize": {
                        "description": "Specifies paper size.",
                        "type": "string",
                        "example": "LETTER",
                        "enum": [
                            "A0",
                            "A1",
                            "A2",
                            "A3",
                            "A4",
                            "ARCHID",
                            "LEGAL",
                            "LETTER",
                            "WIDE"
                        ],
                        "path": "formatDetails.paperSize"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "format": {
                        "type": "text",
                        "index": 1,
                        "label": "Format",
                        "tooltip": "<p>One of the following values: <strong>EXCEL</strong>, <strong>PDF</strong>, or <strong>PDF_GANTT</strong>.</p>"
                    },
                    "ccMe": {
                        "type": "toggle",
                        "index": 2,
                        "label": "Cc Me",
                        "tooltip": "<p>Indicates whether to send a copy of the email to the sender.</p>"
                    },
                    "message": {
                        "type": "text",
                        "index": 3,
                        "label": "Message",
                        "tooltip": "<p>The message of the email.</p>"
                    },
                    "sendTo": {
                        "type": "textarea",
                        "index": 4,
                        "label": "Send To",
                        "tooltip": "<p>Array of recipients.</p> JSON array. Example: [{\"email\":\"john.doe@smartsheet.com\"}]."
                    },
                    "subject": {
                        "type": "text",
                        "index": 5,
                        "label": "Subject",
                        "tooltip": "<p>The subject of the email.</p>"
                    },
                    "formatDetails|paperSize": {
                        "type": "select",
                        "index": 6,
                        "label": "Format Details Paper Size",
                        "tooltip": "<p>Specifies paper size. Example: LETTER</p>",
                        "options": [
                            {
                                "content": "A0",
                                "value": "A0"
                            },
                            {
                                "content": "A1",
                                "value": "A1"
                            },
                            {
                                "content": "A2",
                                "value": "A2"
                            },
                            {
                                "content": "A3",
                                "value": "A3"
                            },
                            {
                                "content": "A4",
                                "value": "A4"
                            },
                            {
                                "content": "ARCHID",
                                "value": "ARCHID"
                            },
                            {
                                "content": "LEGAL",
                                "value": "LEGAL"
                            },
                            {
                                "content": "LETTER",
                                "value": "LETTER"
                            },
                            {
                                "content": "WIDE",
                                "value": "WIDE"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Version",
                    "value": "version"
                },
                {
                    "label": "Failed Items",
                    "value": "failedItems",
                    "schema": {
                        "description": "Array of BulkItemFailure objects which represents the items that failed to be added or updated. See [Partial Success](/partial-success) for more information. Applicable only for bulk operations that support partial success.",
                        "type": "array",
                        "nullable": true,
                        "items": {
                            "type": "object",
                            "properties": {
                                "rowId": {
                                    "description": "The Id of the row that failed. Applicable only to bulk row operations.",
                                    "type": "number",
                                    "nullable": true
                                },
                                "error": {
                                    "description": "The error caused by the failed item.",
                                    "type": "object",
                                    "properties": {
                                        "refId": {
                                            "description": "The Id of the specific error occurrence. Please include this information when contacting Smartsheet support.",
                                            "type": "string"
                                        },
                                        "errorCode": {
                                            "description": "Custom error code from Smartsheet. See the complete [Error Code List](../#section/Error-Code-List).",
                                            "type": "number"
                                        },
                                        "message": {
                                            "description": "Descriptive error message.",
                                            "type": "string"
                                        }
                                    }
                                },
                                "index": {
                                    "description": "The index of the failed item in the bulk request array.",
                                    "type": "number"
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}