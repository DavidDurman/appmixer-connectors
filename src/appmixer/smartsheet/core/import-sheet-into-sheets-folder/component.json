{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.import-sheet-into-sheets-folder",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Import Sheet from CSV / XLSX</p></label><br/><p>Imports CSV or XLSX data into a new sheet in the top-level \"Sheets\" folder.</p>\n<p>Note the following:</p>\n<ul>\n<li>Both sheetName and the file name must use ASCII characters.</li>\n<li>The source data must be basic text. To include rich formula data, import and create a sheet first, and then use Update Rows. To work with images, see Cell Images.</li>\n<li>XLS is not supported. You must use XLSX.</li>\n<li>Hierarchical relationships between rows in an external file won't import.</li>\n</ul>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "required": [
                    "Content-Type",
                    "sheetName"
                ],
                "properties": {
                    "Content-Disposition": {
                        "type": "string",
                        "example": "attachment; filename=\"ProgressReport.docx\""
                    },
                    "Content-Type": {
                        "type": "string",
                        "enum": [
                            "text/csv",
                            "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
                        ]
                    },
                    "sheetName": {
                        "type": "string"
                    },
                    "headerRowIndex": {
                        "type": "number"
                    },
                    "primaryColumnIndex": {
                        "type": "number",
                        "default": 0
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Disposition": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Disposition",
                        "tooltip": "<p>Should be equal to \"attachment\" to tell the API that a file is in the body of the POST request, followed by a semicolon, followed by <strong>filename=</strong> and the URL-encoded filename in quotes</p>"
                    },
                    "Content-Type": {
                        "type": "select",
                        "index": 1,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST request to import a sheet from CSV/XLSX file.</p>\n<ul>\n<li>For CSV files, use: Content-Type: text/csv</li>\n<li>For XLSX files, use: Content-Type: application/vnd.openxmlformats-officedocument.spreadsheetml.sheet</li>\n</ul>",
                        "options": [
                            {
                                "content": "text/csv",
                                "value": "text/csv"
                            },
                            {
                                "content": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
                                "value": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
                            }
                        ]
                    },
                    "sheetName": {
                        "type": "text",
                        "index": 2,
                        "label": "Sheet Name",
                        "tooltip": "<p>Desired name of the sheet.</p>"
                    },
                    "headerRowIndex": {
                        "type": "number",
                        "index": 3,
                        "label": "Header Row Index",
                        "tooltip": "<p>A zero-based integer indicating the row number to use for column names. Rows before this are omitted.\nIf not specified, the default values are Column1, Column2, etc.</p>"
                    },
                    "primaryColumnIndex": {
                        "type": "number",
                        "index": 4,
                        "label": "Primary Column Index",
                        "tooltip": "<p>A zero-based integer indicating the column to designate as primary.</p>",
                        "defaultValue": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Result",
                    "value": "result"
                },
                {
                    "label": "Result Access Level",
                    "value": "result.accessLevel"
                },
                {
                    "label": "Result Id",
                    "value": "result.id"
                },
                {
                    "label": "Result Name",
                    "value": "result.name"
                },
                {
                    "label": "Result Type",
                    "value": "result.type"
                },
                {
                    "label": "Result Permalink",
                    "value": "result.permalink"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}