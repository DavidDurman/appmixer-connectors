                    "value": "result.status"
                },
                {
                    "label": "Result Subscope",
                    "value": "result.subscope"
                },
                {
                    "label": "Result Subscope Column Ids",
                    "value": "result.subscope.columnIds",
                    "schema": {
                        "description": "A column Id or Ids",
                        "type": "array",
                        "items": {
                            "type": "string",
                            "example": "7318427511613316, 7318427511613123"
                        }
                    }
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
