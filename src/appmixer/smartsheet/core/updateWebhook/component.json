                    "value": "result.events",
                    "schema": {
                        "description": "Array of the events that are subscribed to. Currently, must be an array of size 1 that contains the string value '\\*.\\*' (asterisk period asterisk), which means \"all objects\" and \"all events\".",
                        "type": "array",
                        "items": {
                            "type": "string",
                            "example": "*.*"
                        }
                    }
                },
                {
                    "label": "Result Name",
                    "value": "result.name"
                },
                {
                    "label": "Result Version",
                    "value": "result.version"
                },
                {
                    "label": "Result Id",
                    "value": "result.id"
                },
                {
                    "label": "Result Api Client Id",
                    "value": "result.apiClientId"
                },
                {
                    "label": "Result Scope Object Id",
                    "value": "result.scopeObjectId"
                },
                {
                    "label": "Result Api Client Name",
                    "value": "result.apiClientName"
                },
                {
                    "label": "Result Created At",
                    "value": "result.createdAt"
                },
                {
                    "label": "Result Disabled Details",
                    "value": "result.disabledDetails"
                },
                {
                    "label": "Result Modified At",
                    "value": "result.modifiedAt"
                },
                {
                    "label": "Result Scope",
                    "value": "result.scope"
                },
                {
                    "label": "Result Shared Secret",
                    "value": "result.sharedSecret"
                },
                {
                    "label": "Result Stats",
                    "value": "result.stats"
                },
                {
