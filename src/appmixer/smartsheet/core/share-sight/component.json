{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.share-sight",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Share Dashboard</p></label><br/><p>Shares a dashboard with the specified users and groups.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "sendEmail": {
                        "type": "boolean",
                        "default": false
                    },
                    "id": {
                        "description": "Share Id.\n\n**NOTE**: unlike other Smartsheet object Ids, this Id is an alphanumeric string.\n",
                        "type": "string",
                        "path": "id"
                    },
                    "groupId": {
                        "description": "Group Id if the share is a group share, else null.",
                        "type": "number",
                        "path": "groupId"
                    },
                    "userId": {
                        "description": "User Id if the share is a user share, else null.",
                        "type": "number",
                        "path": "userId"
                    },
                    "type": {
                        "description": "The type of this share. One of the following values: GROUP or USER.\n",
                        "type": "string",
                        "path": "type"
                    },
                    "accessLevel": {
                        "type": "string",
                        "enum": [
                            "ADMIN",
                            "COMMENTER",
                            "EDITOR",
                            "EDITOR_SHARE",
                            "OWNER",
                            "VIEWER"
                        ],
                        "path": "accessLevel"
                    },
                    "ccMe": {
                        "description": "Indicates whether to send a copy of the email to the sharer of the sheet. You can specify this attribute\nin a request, but it is never present in a response.\n",
                        "type": "boolean",
                        "path": "ccMe"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "path": "createdAt"
                    },
                    "email": {
                        "description": "User's primary email address for user shares; null for group shares.",
                        "type": "string",
                        "path": "email"
                    },
                    "message": {
                        "description": "The message included in the body of the email that is optionally sent to the recipient. You can specify\nthis attribute in a request, but it is never present in a response.\n",
                        "type": "string",
                        "path": "message"
                    },
                    "modifiedAt": {
                        "type": "string",
                        "format": "date-time",
                        "path": "modifiedAt"
                    },
                    "name": {
                        "description": "If a user share and user is also a contact, the user's full name. If a group share, the group's name.",
                        "type": "string",
                        "path": "name"
                    },
                    "scope": {
                        "description": "The scope of this share. One of the following values:\n  * **ITEM**: an item-level share (that is, the specific object\nto which the share applies is shared with\n    the user or group).\n  * **WORKSPACE**: a workspace-level share (that is, the workspace\nthat contains the object to which the\n    share applies is shared with the user or group).\n",
                        "type": "string",
                        "path": "scope"
                    },
                    "subject": {
                        "description": "The subject of the email that is optionally sent to notify the recipient. You can specify this attribute\nin a request, but it is never present in a response.\n",
                        "type": "string",
                        "path": "subject"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "sendEmail": {
                        "type": "toggle",
                        "index": 0,
                        "label": "Send Email",
                        "tooltip": "<p>Either true or false to indicate whether to notify the user by email. Default is false.</p>",
                        "defaultValue": false
                    },
                    "id": {
                        "type": "text",
                        "index": 1,
                        "label": "Id",
                        "tooltip": "<p>Share Id.</p>\n<p><strong>NOTE</strong>: unlike other Smartsheet object Ids, this Id is an alphanumeric string.</p>"
                    },
                    "groupId": {
                        "type": "number",
                        "index": 2,
                        "label": "Group Id",
                        "tooltip": "<p>Group Id if the share is a group share, else null.</p>"
                    },
                    "userId": {
                        "type": "number",
                        "index": 3,
                        "label": "User Id",
                        "tooltip": "<p>User Id if the share is a user share, else null.</p>"
                    },
                    "type": {
                        "type": "text",
                        "index": 4,
                        "label": "Type",
                        "tooltip": "<p>The type of this share. One of the following values: GROUP or USER.</p>"
                    },
                    "accessLevel": {
                        "type": "select",
                        "index": 5,
                        "label": "Access Level",
                        "tooltip": "",
                        "options": [
                            {
                                "content": "ADMIN",
                                "value": "ADMIN"
                            },
                            {
                                "content": "COMMENTER",
                                "value": "COMMENTER"
                            },
                            {
                                "content": "EDITOR",
                                "value": "EDITOR"
                            },
                            {
                                "content": "EDITOR_SHARE",
                                "value": "EDITOR_SHARE"
                            },
                            {
                                "content": "OWNER",
                                "value": "OWNER"
                            },
                            {
                                "content": "VIEWER",
                                "value": "VIEWER"
                            }
                        ]
                    },
                    "ccMe": {
                        "type": "toggle",
                        "index": 6,
                        "label": "Cc Me",
                        "tooltip": "<p>Indicates whether to send a copy of the email to the sharer of the sheet. You can specify this attribute\nin a request, but it is never present in a response.</p>"
                    },
                    "createdAt": {
                        "type": "date-time",
                        "index": 7,
                        "label": "Created At",
                        "tooltip": ""
                    },
                    "email": {
                        "type": "text",
                        "index": 8,
                        "label": "Email",
                        "tooltip": "<p>User's primary email address for user shares; null for group shares.</p>"
                    },
                    "message": {
                        "type": "text",
                        "index": 9,
                        "label": "Message",
                        "tooltip": "<p>The message included in the body of the email that is optionally sent to the recipient. You can specify\nthis attribute in a request, but it is never present in a response.</p>"
                    },
                    "modifiedAt": {
                        "type": "date-time",
                        "index": 10,
                        "label": "Modified At",
                        "tooltip": ""
                    },
                    "name": {
                        "type": "text",
                        "index": 11,
                        "label": "Name",
                        "tooltip": "<p>If a user share and user is also a contact, the user's full name. If a group share, the group's name.</p>"
                    },
                    "scope": {
                        "type": "text",
                        "index": 12,
                        "label": "Scope",
                        "tooltip": "<p>The scope of this share. One of the following values:</p>\n<ul>\n<li><strong>ITEM</strong>: an item-level share (that is, the specific object\nto which the share applies is shared with\nthe user or group).</li>\n<li><strong>WORKSPACE</strong>: a workspace-level share (that is, the workspace\nthat contains the object to which the\nshare applies is shared with the user or group).</li>\n</ul>"
                    },
                    "subject": {
                        "type": "text",
                        "index": 13,
                        "label": "Subject",
                        "tooltip": "<p>The subject of the email that is optionally sent to notify the recipient. You can specify this attribute\nin a request, but it is never present in a response.</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Version",
                    "value": "version"
                },
                {
                    "label": "Failed Items",
                    "value": "failedItems",
                    "schema": {
                        "description": "Array of BulkItemFailure objects which represents the items that failed to be added or updated. See [Partial Success](/partial-success) for more information. Applicable only for bulk operations that support partial success.",
                        "type": "array",
                        "nullable": true,
                        "items": {
                            "type": "object",
                            "properties": {
                                "rowId": {
                                    "description": "The Id of the row that failed. Applicable only to bulk row operations.",
                                    "type": "number",
                                    "nullable": true
                                },
                                "error": {
                                    "description": "The error caused by the failed item.",
                                    "type": "object",
                                    "properties": {
                                        "refId": {
                                            "description": "The Id of the specific error occurrence. Please include this information when contacting Smartsheet support.",
                                            "type": "string"
                                        },
                                        "errorCode": {
                                            "description": "Custom error code from Smartsheet. See the complete [Error Code List](../#section/Error-Code-List).",
                                            "type": "number"
                                        },
                                        "message": {
                                            "description": "Descriptive error message.",
                                            "type": "string"
                                        }
                                    }
                                },
                                "index": {
                                    "description": "The index of the failed item in the bulk request array.",
                                    "type": "number"
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Result",
                    "value": "result"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}