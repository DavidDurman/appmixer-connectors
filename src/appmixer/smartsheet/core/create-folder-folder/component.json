{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.create-folder-folder",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Create Folder</p></label><br/><p>Creates a new folder.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "include": {
                        "type": "string",
                        "enum": [
                            "attachments",
                            "cellLinks",
                            "data",
                            "discussions",
                            "filters",
                            "forms",
                            "ruleRecipients",
                            "rules",
                            "shares"
                        ]
                    },
                    "exclude": {
                        "type": "string",
                        "enum": [
                            "sheetHyperlinks"
                        ]
                    },
                    "skipRemap": {
                        "type": "string",
                        "enum": [
                            "cellLinks",
                            "reports",
                            "sheetHyperlinks",
                            "sights"
                        ]
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "include": {
                        "type": "select",
                        "index": 1,
                        "label": "Include",
                        "tooltip": "<p>A comma-separated list of elements to copy:</p>\n<ul>\n<li><strong>attachments</strong></li>\n<li><strong>cellLinks</strong> - includes cross-sheet references</li>\n<li><strong>data</strong> - includes formatting</li>\n<li><strong>discussions</strong> - includes comments</li>\n<li><strong>filters</strong></li>\n<li><strong>forms</strong></li>\n<li><strong>ruleRecipients</strong> -- includes notification recipients, must also include\nrules when using this attribute</li>\n<li><strong>rules</strong> -- includes notifications and workflow rules</li>\n<li><strong>shares</strong> |\nNOTE: Cell history is not copied, regardless of which include parameter values are specified.</li>\n</ul>",
                        "options": [
                            {
                                "content": "attachments",
                                "value": "attachments"
                            },
                            {
                                "content": "cellLinks",
                                "value": "cellLinks"
                            },
                            {
                                "content": "data",
                                "value": "data"
                            },
                            {
                                "content": "discussions",
                                "value": "discussions"
                            },
                            {
                                "content": "filters",
                                "value": "filters"
                            },
                            {
                                "content": "forms",
                                "value": "forms"
                            },
                            {
                                "content": "ruleRecipients",
                                "value": "ruleRecipients"
                            },
                            {
                                "content": "rules",
                                "value": "rules"
                            },
                            {
                                "content": "shares",
                                "value": "shares"
                            }
                        ]
                    },
                    "exclude": {
                        "type": "select",
                        "index": 2,
                        "label": "Exclude",
                        "tooltip": "<p>When specified with a value of <strong>sheetHyperlinks</strong>, excludes this category from the response</p>",
                        "options": [
                            {
                                "content": "sheetHyperlinks",
                                "value": "sheetHyperlinks"
                            }
                        ]
                    },
                    "skipRemap": {
                        "type": "select",
                        "index": 3,
                        "label": "Skip Remap",
                        "tooltip": "<p>A comma-separated list of references to NOT re-map for the newly created folder.</p>",
                        "options": [
                            {
                                "content": "cellLinks",
                                "value": "cellLinks"
                            },
                            {
                                "content": "reports",
                                "value": "reports"
                            },
                            {
                                "content": "sheetHyperlinks",
                                "value": "sheetHyperlinks"
                            },
                            {
                                "content": "sights",
                                "value": "sights"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Result",
                    "value": "result"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}