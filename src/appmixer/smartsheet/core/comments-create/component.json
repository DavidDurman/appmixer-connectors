{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.comments-create",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Create a comment</p></label><br/><p>Adds a comment to a discussion. To create a comment with an attachment please use \"multipart/form-data\" content type.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "text": {
                        "description": "Comment body.",
                        "type": "string",
                        "path": "text"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "text": {
                        "type": "text",
                        "index": 1,
                        "label": "Text",
                        "tooltip": "<p>Comment body.</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Result",
                    "value": "result"
                },
                {
                    "label": "Result Attachments",
                    "value": "result.attachments",
                    "schema": {
                        "description": "Array of attachments on comments.",
                        "type": "array",
                        "items": {
                            "description": "Attachment Object",
                            "type": "object",
                            "properties": {
                                "id": {
                                    "description": "Attachment Id.",
                                    "type": "number"
                                },
                                "parentId": {
                                    "description": "The Id of the parent.",
                                    "type": "number"
                                },
                                "attachmentType": {
                                    "description": "Attachment type. * types are not supported for Smartsheet.gov accounts.",
                                    "type": "string",
                                    "enum": [
                                        "BOX_COM",
                                        "DROPBOX*",
                                        "EGNYTE*",
                                        "EVERNOTE*",
                                        "FILE",
                                        "GOOGLE_DRIVE",
                                        "LINK",
                                        "ONEDRIVE"
                                    ]
                                },
                                "attachmentSubType": {
                                    "description": "Attachment sub type. * type is for EGNYTE values and the rest are GOOGLE_DRIVE values.",
                                    "type": "string",
                                    "enum": [
                                        "DOCUMENT",
                                        "DRAWING",
                                        "FOLDER*",
                                        "PDF",
                                        "PRESENTATION",
                                        "SPREADSHEET"
                                    ]
                                },
                                "mimeType": {
                                    "description": "Attachment MIME type.",
                                    "type": "string",
                                    "example": "PNG"
                                },
                                "parentType": {
                                    "description": "The type of object the attachment belongs to.",
                                    "type": "string",
                                    "enum": [
                                        "COMMENT",
                                        "PROOF",
                                        "ROW",
                                        "SHEET"
                                    ]
                                },
                                "createdAt": {
                                    "oneOf": [
                                        {
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        {
                                            "type": "number"
                                        }
                                    ],
                                    "readOnly": true
                                },
                                "createdBy": {
                                    "description": "`User` object containing `name` and `email` of the user who created this attachment.\n",
                                    "type": "object",
                                    "properties": {
                                        "email": {
                                            "type": "string",
                                            "format": "email",
                                            "example": "jane.doe@smartsheet.com"
                                        },
                                        "name": {
                                            "type": "string",
                                            "readOnly": true,
                                            "example": "Jane Doe"
                                        }
                                    }
                                },
                                "name": {
                                    "description": "Attachment name.",
                                    "type": "string"
                                },
                                "sizeInKb": {
                                    "description": "The size of the file, if the attachmentType is FILE.",
                                    "type": "number"
                                },
                                "url": {
                                    "description": "Attachment temporary URL (files only).",
                                    "type": "string"
                                },
                                "urlExpiresInMillis": {
                                    "description": "Attachment temporary URL time to live (files only).",
                                    "type": "number"
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Result Created At",
                    "value": "result.createdAt"
                },
                {
                    "label": "Result Created By",
                    "value": "result.createdBy"
                },
                {
                    "label": "Result Created By Email",
                    "value": "result.createdBy.email"
                },
                {
                    "label": "Result Created By Name",
                    "value": "result.createdBy.name"
                },
                {
                    "label": "Result Discussion Id",
                    "value": "result.discussionId"
                },
                {
                    "label": "Result Id",
                    "value": "result.id"
                },
                {
                    "label": "Result Modified At",
                    "value": "result.modifiedAt"
                },
                {
                    "label": "Result Text",
                    "value": "result.text"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}