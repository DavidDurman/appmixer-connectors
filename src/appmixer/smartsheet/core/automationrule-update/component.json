{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.automationrule-update",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Update an Automation Rule</p></label><br/><p>Updates an existing automation rule.</p>\n<p>When sending an AutomationRule, you must always specify <strong>action.type</strong> and it must match the existing rule type.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "id": {
                        "description": "AutomationRule Id.",
                        "type": "number",
                        "path": "id"
                    },
                    "action": {
                        "path": "action",
                        "properties": {
                            "notifyAllSharedUsers": {
                                "description": "If true, notifications are sent to all users shared to the sheet.",
                                "type": "boolean"
                            }
                        }
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "path": "createdAt"
                    },
                    "disabledReason": {
                        "description": "Machine-readable reason a rule is disabled:\n* `APPROVAL_COLUMN_MISSING` This rule's approval status column has been deleted.\n\n* `APPROVAL_COLUMN_WRONG_TYPE` The approval column must be a dropdown column.\n\n* `AUTOMATION_NOT_ENABLED_FOR_ORG` To create or edit automated actions,\nyou need to upgrade your organization account to a Business or Enterprise plan.\n\n* `COLUMN_MISSING` A column referenced by this rule has been deleted.\n\n* `COLUMN_TYPE_INCOMPATIBLE` A column referenced by this rule has been changed to an incompatible column type.\n\n* `NO_POTENTIAL_RECIPIENTS` This rule has no recipients that will be able to receive\nnotifications based on this sheet's permission settings or this account's approved domain sharing list.\n\n* `NO_VALID_SELECTED_COLUMNS` All selected columns for this rule have been deleted.\n",
                        "type": "string",
                        "enum": [
                            "APPROVAL_COLUMN_MISSING",
                            "APPROVAL_COLUMN_WRONG_TYPE",
                            "AUTOMATION_NOT_ENABLED_FOR_ORG",
                            "COLUMN_MISSING",
                            "COLUMN_TYPE_INCOMPATIBLE",
                            "NO_POTENTIAL_RECIPIENTS",
                            "NO_VALID_SELECTED_COLUMNS"
                        ],
                        "path": "disabledReason"
                    },
                    "disabledReasonText": {
                        "description": "Descriptive reason a rule is disabled.",
                        "type": "string",
                        "path": "disabledReasonText"
                    },
                    "enabled": {
                        "description": "If true, indicates that the rule is active.",
                        "type": "boolean",
                        "path": "enabled"
                    },
                    "modifiedAt": {
                        "type": "string",
                        "format": "date-time",
                        "path": "modifiedAt"
                    },
                    "name": {
                        "description": "Rule name as shown in the UI.",
                        "type": "string",
                        "path": "name"
                    },
                    "userCanModify": {
                        "description": "If true, indicates that the current user can modify the rule.",
                        "type": "boolean",
                        "path": "userCanModify"
                    },
                    "createdBy|email": {
                        "type": "string",
                        "format": "email",
                        "example": "jane.doe@smartsheet.com",
                        "path": "createdBy.email"
                    },
                    "createdBy|name": {
                        "type": "string",
                        "readOnly": true,
                        "example": "Jane Doe",
                        "path": "createdBy.name"
                    },
                    "modifiedBy|email": {
                        "type": "string",
                        "format": "email",
                        "example": "jane.doe@smartsheet.com",
                        "path": "modifiedBy.email"
                    },
                    "modifiedBy|name": {
                        "type": "string",
                        "readOnly": true,
                        "example": "Jane Doe",
                        "path": "modifiedBy.name"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "id": {
                        "type": "number",
                        "index": 1,
                        "label": "Id",
                        "tooltip": "<p>AutomationRule Id.</p>"
                    },
                    "action": {
                        "type": "text",
                        "index": 2,
                        "label": "Action",
                        "tooltip": ""
                    },
                    "createdAt": {
                        "type": "date-time",
                        "index": 3,
                        "label": "Created At",
                        "tooltip": ""
                    },
                    "disabledReason": {
                        "type": "select",
                        "index": 4,
                        "label": "Disabled Reason",
                        "tooltip": "<p>Machine-readable reason a rule is disabled:</p>\n<ul>\n<li><p><code>APPROVAL_COLUMN_MISSING</code> This rule's approval status column has been deleted.</p></li>\n<li><p><code>APPROVAL_COLUMN_WRONG_TYPE</code> The approval column must be a dropdown column.</p></li>\n<li><p><code>AUTOMATION_NOT_ENABLED_FOR_ORG</code> To create or edit automated actions,\nyou need to upgrade your organization account to a Business or Enterprise plan.</p></li>\n<li><p><code>COLUMN_MISSING</code> A column referenced by this rule has been deleted.</p></li>\n<li><p><code>COLUMN_TYPE_INCOMPATIBLE</code> A column referenced by this rule has been changed to an incompatible column type.</p></li>\n<li><p><code>NO_POTENTIAL_RECIPIENTS</code> This rule has no recipients that will be able to receive\nnotifications based on this sheet's permission settings or this account's approved domain sharing list.</p></li>\n<li><p><code>NO_VALID_SELECTED_COLUMNS</code> All selected columns for this rule have been deleted.</p></li>\n</ul>",
                        "options": [
                            {
                                "content": "APPROVAL_COLUMN_MISSING",
                                "value": "APPROVAL_COLUMN_MISSING"
                            },
                            {
                                "content": "APPROVAL_COLUMN_WRONG_TYPE",
                                "value": "APPROVAL_COLUMN_WRONG_TYPE"
                            },
                            {
                                "content": "AUTOMATION_NOT_ENABLED_FOR_ORG",
                                "value": "AUTOMATION_NOT_ENABLED_FOR_ORG"
                            },
                            {
                                "content": "COLUMN_MISSING",
                                "value": "COLUMN_MISSING"
                            },
                            {
                                "content": "COLUMN_TYPE_INCOMPATIBLE",
                                "value": "COLUMN_TYPE_INCOMPATIBLE"
                            },
                            {
                                "content": "NO_POTENTIAL_RECIPIENTS",
                                "value": "NO_POTENTIAL_RECIPIENTS"
                            },
                            {
                                "content": "NO_VALID_SELECTED_COLUMNS",
                                "value": "NO_VALID_SELECTED_COLUMNS"
                            }
                        ]
                    },
                    "disabledReasonText": {
                        "type": "text",
                        "index": 5,
                        "label": "Disabled Reason Text",
                        "tooltip": "<p>Descriptive reason a rule is disabled.</p>"
                    },
                    "enabled": {
                        "type": "toggle",
                        "index": 6,
                        "label": "Enabled",
                        "tooltip": "<p>If true, indicates that the rule is active.</p>"
                    },
                    "modifiedAt": {
                        "type": "date-time",
                        "index": 7,
                        "label": "Modified At",
                        "tooltip": ""
                    },
                    "name": {
                        "type": "text",
                        "index": 8,
                        "label": "Name",
                        "tooltip": "<p>Rule name as shown in the UI.</p>"
                    },
                    "userCanModify": {
                        "type": "toggle",
                        "index": 9,
                        "label": "User Can Modify",
                        "tooltip": "<p>If true, indicates that the current user can modify the rule.</p>"
                    },
                    "createdBy|email": {
                        "type": "text",
                        "index": 10,
                        "label": "Created By Email",
                        "tooltip": "<p>Example: jane.doe@smartsheet.com</p>"
                    },
                    "createdBy|name": {
                        "type": "text",
                        "index": 11,
                        "label": "Created By Name",
                        "tooltip": "<p>Example: Jane Doe</p>"
                    },
                    "modifiedBy|email": {
                        "type": "text",
                        "index": 12,
                        "label": "Modified By Email",
                        "tooltip": "<p>Example: jane.doe@smartsheet.com</p>"
                    },
                    "modifiedBy|name": {
                        "type": "text",
                        "index": 13,
                        "label": "Modified By Name",
                        "tooltip": "<p>Example: Jane Doe</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Version",
                    "value": "version"
                },
                {
                    "label": "Failed Items",
