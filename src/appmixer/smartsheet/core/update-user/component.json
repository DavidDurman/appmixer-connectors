{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.update-user",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Update User</p></label><br/><p>Updates the user specified in the URL.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "admin": {
                        "description": "Indicates whether the user is a system admin (can manage user accounts and organization account).",
                        "type": "boolean",
                        "default": false,
                        "example": true,
                        "path": "admin"
                    },
                    "licensedSheetCreator": {
                        "description": "Indicates whether the user is a licensed user (can create and own sheets).",
                        "type": "boolean",
                        "default": false,
                        "example": true,
                        "path": "licensedSheetCreator"
                    },
                    "firstName": {
                        "description": "User's first name.",
                        "type": "string",
                        "example": "Jane",
                        "path": "firstName"
                    },
                    "lastName": {
                        "description": "User's last name.",
                        "type": "string",
                        "example": "Doe",
                        "path": "lastName"
                    },
                    "groupAdmin": {
                        "description": "Indicates whether the user is a group admin (can create and edit groups).",
                        "type": "boolean",
                        "default": false,
                        "example": true,
                        "path": "groupAdmin"
                    },
                    "resourceViewer": {
                        "description": "Indicates whether the user is a resource viewer (can access resource views).",
                        "type": "boolean",
                        "default": false,
                        "example": true,
                        "path": "resourceViewer"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "admin": {
                        "type": "toggle",
                        "index": 0,
                        "label": "Admin",
                        "tooltip": "<p>Indicates whether the user is a system admin (can manage user accounts and organization account). Example: true</p>",
                        "defaultValue": false
                    },
                    "licensedSheetCreator": {
                        "type": "toggle",
                        "index": 1,
                        "label": "Licensed Sheet Creator",
                        "tooltip": "<p>Indicates whether the user is a licensed user (can create and own sheets). Example: true</p>",
                        "defaultValue": false
                    },
                    "firstName": {
                        "type": "text",
                        "index": 2,
                        "label": "First Name",
                        "tooltip": "<p>User's first name. Example: Jane</p>"
                    },
                    "lastName": {
                        "type": "text",
                        "index": 3,
                        "label": "Last Name",
                        "tooltip": "<p>User's last name. Example: Doe</p>"
                    },
                    "groupAdmin": {
                        "type": "toggle",
                        "index": 4,
                        "label": "Group Admin",
                        "tooltip": "<p>Indicates whether the user is a group admin (can create and edit groups). Example: true</p>",
                        "defaultValue": false
                    },
                    "resourceViewer": {
                        "type": "toggle",
                        "index": 5,
                        "label": "Resource Viewer",
                        "tooltip": "<p>Indicates whether the user is a resource viewer (can access resource views). Example: true</p>",
                        "defaultValue": false
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Data",
                    "value": "data",
                    "schema": {
                        "description": "Updated User Properties",
                        "type": "array",
                        "items": {
                            "description": "User profile image response.",
                            "properties": {
                                "email": {
                                    "description": "User's primary email address.",
                                    "type": "string",
                                    "format": "email",
                                    "example": "jane.doe@smartsheet.com"
                                },
                                "name": {
                                    "description": "User's full name (read-only).",
                                    "type": "string",
                                    "readOnly": true,
                                    "example": "Jane Doe"
                                },
                                "firstName": {
                                    "description": "User's first name.",
                                    "type": "string",
                                    "example": "Jane"
                                },
                                "lastName": {
                                    "description": "User's last name.",
                                    "type": "string",
                                    "example": "Doe"
                                },
                                "profileImage": {
                                    "type": "object",
                                    "properties": {
                                        "imageId": {
                                            "description": "Unique image Id.",
                                            "type": "string",
                                            "example": "u!1!nAtdn5RJB_o!k6_e_3h2R3w!wmYXPek-yVD"
                                        },
                                        "height": {
                                            "description": "Image height.",
                                            "type": "string",
                                            "example": 1050
                                        },
                                        "width": {
                                            "description": "Image width.",
                                            "type": "string",
                                            "example": 1050
                                        }
                                    }
                                },
                                "id": {
                                    "description": "User Id.",
                                    "type": "number",
                                    "readOnly": true,
                                    "example": 48569348493401200
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}