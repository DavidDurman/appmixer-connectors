{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.row-get",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Get Row</p></label><br/><p>Gets the row specified in the URL.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "accessApiLevel": {
                        "type": "number",
                        "default": 0
                    },
                    "include": {
                        "type": "string",
                        "enum": [
                            "columns",
                            "filters"
                        ]
                    },
                    "exclude": {
                        "type": "string",
                        "enum": [
                            "filteredOutRows",
                            "linkInFromCellDetails",
                            "linksOutToCellsDetails",
                            "nonexistentCells"
                        ]
                    },
                    "level": {
                        "type": "integer",
                        "default": 1
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "accessApiLevel": {
                        "type": "number",
                        "index": 0,
                        "label": "Access Api Level",
                        "tooltip": "<p>Allows COMMENTER access for inputs and return values. For backwards-compatibility, VIEWER is the default. For example, to see whether a user has COMMENTER access for a sheet, use accessApiLevel=1.</p>",
                        "defaultValue": 0
                    },
                    "include": {
                        "type": "select",
                        "index": 1,
                        "label": "Include",
                        "tooltip": "<p>A comma-separated list of elements to include in the response.</p>\n<p>See <a href=\"../../tag/rowsRelated#section/Row-Include-Flags\" rel=\"noopener noreferrer\" target=\"_blank\">Row Include Flags</a>.</p>\n<p>Also supports the <strong>columns</strong> include flag, which adds a columns array that specifies all of the columns for the sheet. This enables you to have the full context of the cells in the row.</p>\n<p>The <strong>filters</strong> include flag returns a <strong>filteredOut</strong> attribute indicating if the row should be displayed or hidden according to the sheet's filters.</p>",
                        "options": [
                            {
                                "content": "columns",
                                "value": "columns"
                            },
                            {
                                "content": "filters",
                                "value": "filters"
                            }
                        ]
                    },
                    "exclude": {
                        "type": "select",
                        "index": 2,
                        "label": "Exclude",
                        "tooltip": "<p>A comma-separated list of optional elements to not include in the response:</p>\n<ul>\n<li><strong>filteredOutRows</strong> - excludes filtered out rows from response payload\nif a sheet filter is applied; includes total number of filtered rows</li>\n<li><strong>linkInFromCellDetails</strong> - excludes the following attributes from the\n<strong>cell.linkInFromCell</strong> object: columnId, rowId, status</li>\n<li><strong>linksOutToCellsDetails</strong> - excludes the following attributes from\nthe <strong>cell.linksOutToCells</strong> array elements: columnId, rowId, status</li>\n<li><strong>nonexistentCells</strong> - excludes cells that have never contained any\ndata</li>\n</ul>",
                        "options": [
                            {
                                "content": "filteredOutRows",
                                "value": "filteredOutRows"
                            },
                            {
                                "content": "linkInFromCellDetails",
                                "value": "linkInFromCellDetails"
                            },
                            {
                                "content": "linksOutToCellsDetails",
                                "value": "linksOutToCellsDetails"
                            },
                            {
                                "content": "nonexistentCells",
                                "value": "nonexistentCells"
                            }
                        ]
                    },
                    "level": {
                        "type": "number",
                        "index": 3,
                        "label": "Level",
                        "tooltip": "<p>Specifies whether new functionality, such as multi-contact data is returned in a backwards-compatible, text format (level=0, default), multi-contact data (level=1), or multi-picklist data (level=2).</p>",
                        "defaultValue": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id"
                },
                {
                    "label": "Sheet Id",
                    "value": "sheetId"
                },
                {
                    "label": "Row Number",
                    "value": "rowNumber"
                },
                {
                    "label": "Expanded",
                    "value": "expanded"
                },
                {
                    "label": "Cells",
                    "value": "cells",
                    "schema": {
                        "description": "Cells objects.",
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "columnId": {
                                    "type": "number",
                                    "description": "The Id of the column that the cell is located in."
                                },
                                "columnType": {
                                    "type": "string",
                                    "description": "Only returned if the include query string parameter contains **columnType**."
                                },
                                "value": {
                                    "anyOf": [
                                        {
                                            "type": "string"
                                        },
                                        {
                                            "type": "number"
                                        },
                                        {
                                            "type": "boolean"
                                        }
                                    ],
                                    "description": "A string, number, or a Boolean value -- depending on the cell type and the data in the cell. Cell values larger than 4000 characters are silently truncated. An empty cell returns no value."
                                },
                                "displayValue": {
                                    "type": "string",
                                    "description": "Visual representation of cell contents, as presented to the user in the UI."
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Created At",
                    "value": "createdAt"
                },
                {
                    "label": "Modified At",
                    "value": "modifiedAt"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}