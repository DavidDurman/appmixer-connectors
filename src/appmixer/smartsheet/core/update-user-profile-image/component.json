{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.update-user-profile-image",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Update User Profile Image</p></label><br/><p>Uploads an image to the user profile.</p>\n<p>Uploading a profile image differs from Adding an Image to a Cell in the following ways:</p>\n<ul>\n<li>A <strong>Content-Length</strong> header is not required</li>\n<li>Allowable file types are limited to: gif, jpg, and png</li>\n<li>Maximum file size is determined by the following rules:<ul>\n<li>If you have not defined a custom size and the image is larger than\n1050 x 1050 pixels, Smartsheet scales the image down to 1050 x 1050</li>\n<li>If you have defined a custom size, Smartsheet uses that as the file\nsize max</li></ul></li>\n<li>If the image is not square, Smartsheet uses a solid color to pad the image</li>\n</ul>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "attachmentSubType": {
                        "description": "Attachment sub type. * type is for EGNYTE values and the rest are GOOGLE_DRIVE values.",
                        "type": "string",
                        "enum": [
                            "DOCUMENT",
                            "DRAWING",
                            "FOLDER*",
                            "PDF",
                            "PRESENTATION",
                            "SPREADSHEET"
                        ],
                        "path": "attachmentSubType"
                    },
                    "attachmentType": {
                        "description": "Attachment type. * types are not supported for Smartsheet.gov accounts.",
                        "type": "string",
                        "enum": [
                            "BOX_COM",
                            "DROPBOX*",
                            "EGNYTE*",
                            "EVERNOTE*",
                            "FILE",
                            "GOOGLE_DRIVE",
                            "LINK",
                            "ONEDRIVE"
                        ],
                        "path": "attachmentType"
                    },
                    "description": {
                        "type": "string",
                        "path": "description"
                    },
                    "name": {
                        "type": "string",
                        "path": "name"
                    },
                    "url": {
                        "type": "string",
                        "path": "url"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "attachmentSubType": {
                        "type": "select",
                        "index": 1,
                        "label": "Attachment Sub Type",
                        "tooltip": "<p>Attachment sub type. * type is for EGNYTE values and the rest are GOOGLE_DRIVE values.</p>",
                        "options": [
                            {
                                "content": "DOCUMENT",
                                "value": "DOCUMENT"
                            },
                            {
                                "content": "DRAWING",
                                "value": "DRAWING"
                            },
                            {
                                "content": "FOLDER*",
                                "value": "FOLDER*"
                            },
                            {
                                "content": "PDF",
                                "value": "PDF"
                            },
                            {
                                "content": "PRESENTATION",
                                "value": "PRESENTATION"
                            },
                            {
                                "content": "SPREADSHEET",
                                "value": "SPREADSHEET"
                            }
                        ]
                    },
                    "attachmentType": {
                        "type": "select",
                        "index": 2,
                        "label": "Attachment Type",
                        "tooltip": "<p>Attachment type. * types are not supported for Smartsheet.gov accounts.</p>",
                        "options": [
                            {
                                "content": "BOX_COM",
                                "value": "BOX_COM"
                            },
                            {
                                "content": "DROPBOX*",
                                "value": "DROPBOX*"
                            },
                            {
                                "content": "EGNYTE*",
                                "value": "EGNYTE*"
                            },
                            {
                                "content": "EVERNOTE*",
                                "value": "EVERNOTE*"
                            },
                            {
                                "content": "FILE",
                                "value": "FILE"
                            },
                            {
                                "content": "GOOGLE_DRIVE",
                                "value": "GOOGLE_DRIVE"
                            },
                            {
                                "content": "LINK",
                                "value": "LINK"
                            },
                            {
                                "content": "ONEDRIVE",
                                "value": "ONEDRIVE"
                            }
                        ]
                    },
                    "description": {
                        "type": "text",
                        "index": 3,
                        "label": "Description",
                        "tooltip": ""
                    },
                    "name": {
                        "type": "text",
                        "index": 4,
                        "label": "Name",
                        "tooltip": ""
                    },
                    "url": {
                        "type": "text",
                        "index": 5,
                        "label": "Url",
                        "tooltip": ""
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Data",
                    "value": "data",
                    "schema": {
                        "description": "Updated User Properties",
                        "type": "array",
                        "items": {
                            "description": "User profile image response.",
                            "properties": {
                                "email": {
                                    "description": "User's primary email address.",
                                    "type": "string",
                                    "format": "email",
                                    "example": "jane.doe@smartsheet.com"
                                },
                                "name": {
                                    "description": "User's full name (read-only).",
                                    "type": "string",
                                    "readOnly": true,
                                    "example": "Jane Doe"
                                },
                                "firstName": {
                                    "description": "User's first name.",
                                    "type": "string",
                                    "example": "Jane"
                                },
                                "lastName": {
                                    "description": "User's last name.",
                                    "type": "string",
                                    "example": "Doe"
                                },
                                "profileImage": {
                                    "type": "object",
                                    "properties": {
                                        "imageId": {
                                            "description": "Unique image Id.",
                                            "type": "string",
                                            "example": "u!1!nAtdn5RJB_o!k6_e_3h2R3w!wmYXPek-yVD"
                                        },
                                        "height": {
                                            "description": "Image height.",
                                            "type": "string",
                                            "example": 1050
                                        },
                                        "width": {
                                            "description": "Image width.",
                                            "type": "string",
                                            "example": 1050
                                        }
                                    }
                                },
                                "id": {
                                    "description": "User Id.",
                                    "type": "number",
                                    "readOnly": true,
                                    "example": 48569348493401200
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}