{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.copy-rows",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Copy Rows to Another Sheet</p></label><br/><p>Copies rows from the sheet specified in the URL to (the bottom of) another sheet.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "include": {
                        "type": "string",
                        "enum": [
                            "all",
                            "attachments",
                            "children",
                            "discussions"
                        ]
                    },
                    "ignoreRowsNotFound": {
                        "type": "boolean",
                        "default": false
                    },
                    "rowIds": {
                        "description": "The Ids of the rows to move or copy from the source sheet.",
                        "type": "array",
                        "path": "rowIds",
                        "items": {
                            "type": "number"
                        }
                    },
                    "to|sheetId": {
                        "description": "The Id of the destination sheet.",
                        "type": "number",
                        "path": "to.sheetId"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "include": {
                        "type": "select",
                        "index": 1,
                        "label": "Include",
                        "tooltip": "<p>A comma-separated list of row elements to copy in addition to the cell data:</p>\n<ul>\n<li><strong>all</strong> - specify a value of <strong>all</strong> to include everything (attachments,\nchildren, and discussions).</li>\n<li><strong>attachments</strong></li>\n<li><strong>children</strong> -  if specified, any child rows of the rows specified in\nthe request are also copied to the destination sheet, and parent-child relationships amongst rows are preserved within the destination sheet; if not specified, only the rows specified in the request are copied.</li>\n<li><strong>discussions</strong></li>\n</ul>",
                        "options": [
                            {
                                "content": "all",
                                "value": "all"
                            },
                            {
                                "content": "attachments",
                                "value": "attachments"
                            },
                            {
                                "content": "children",
                                "value": "children"
                            },
                            {
                                "content": "discussions",
                                "value": "discussions"
                            }
                        ]
                    },
                    "ignoreRowsNotFound": {
                        "type": "toggle",
                        "index": 2,
                        "label": "Ignore Rows Not Found",
                        "tooltip": "<p><strong>true</strong> or <strong>false</strong>: default is <strong>false</strong>. If set to <strong>true</strong>, specifying row Ids that do not exist within the source sheet does not cause an error response. If omitted or set to <strong>false</strong>, specifying row Ids that do not exist within the source sheet causes an error response (and no rows are copied).</p>",
                        "defaultValue": false
                    },
                    "rowIds": {
                        "type": "textarea",
                        "index": 3,
                        "label": "Row Ids",
                        "tooltip": "<p>The Ids of the rows to move or copy from the source sheet.</p> JSON array. Example: [-78725430.78646381]."
                    },
                    "to|sheetId": {
                        "type": "number",
                        "index": 4,
                        "label": "To Sheet Id",
                        "tooltip": "<p>The Id of the destination sheet.</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Destination Sheet Id",
                    "value": "destinationSheetId"
                },
                {
                    "label": "Row Mappings",
                    "value": "rowMappings",
                    "schema": {
                        "description": "Array of rowMapping objects.",
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "from": {
                                    "description": "Row Id in the source sheet.",
                                    "type": "number"
                                },
                                "to": {
                                    "description": "Row Id in the destination sheet.",
                                    "type": "number"
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}