{
    "version": "1.0.0",
    "name": "appmixer.smartsheetapireference.core.set-sheetPublish",
    "author": "Appmixer <info@appmixer.com>",
    "description": "<label><p>Set Sheet Publish Status</p></label><br/><p>Sets the publish status of the sheet and returns the new status, including the URLs of any enabled publishings.</p>",
    "private": false,
    "quota": {},
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "Content-Type": {
                        "type": "string",
                        "default": "application/json"
                    },
                    "icalEnabled": {
                        "description": "If **true**, a webcal is available for the calendar in the sheet.",
                        "type": "boolean",
                        "path": "icalEnabled"
                    },
                    "readOnlyFullAccessibleBy": {
                        "description": "Indicates who can access the 'Read-Only Full' view of the published sheet:\n  * ALL - available to anyone who has the link.\n  * ORG - available only to members of the sheet owner's Smartsheet\norganization account.\n  * SHARED - available only to users shared to the item.\n\nOnly returned in the response if **readOnlyFullEnabled = true**.\n",
                        "type": "string",
                        "enum": [
                            "ALL",
                            "ORG",
                            "SHARED"
                        ],
                        "path": "readOnlyFullAccessibleBy"
                    },
                    "readOnlyFullDefaultView": {
                        "description": "Indicates which view the user has set for a read-only, default view of the published sheet. Must be one of the listed enum values.\n",
                        "type": "string",
                        "enum": [
                            "CALENDAR",
                            "CARD",
                            "GRID"
                        ],
                        "path": "readOnlyFullDefaultView"
                    },
                    "readOnlyFullEnabled": {
                        "description": "If **true**, a rich version of the sheet is published with the ability to download row attachments and discussions.",
                        "type": "boolean",
                        "path": "readOnlyFullEnabled"
                    },
                    "readOnlyFullShowToolbar": {
                        "description": "Indicates whether the left nav toolbar is displayed. The default, or **true**, is to display the toolbar. If **false**, hides the toolbar.",
                        "type": "boolean",
                        "deprecated": true,
                        "path": "readOnlyFullShowToolbar"
                    },
                    "readOnlyLiteEnabled": {
                        "description": "If **true**, a lightweight version of the sheet is published without row attachments and discussions.",
                        "type": "boolean",
                        "path": "readOnlyLiteEnabled"
                    },
                    "readWriteAccessibleBy": {
                        "description": "Indicates who can access the 'Edit by Anyone' view of the published sheet:\n  * ALL - available to anyone who has the link.\n  * ORG - available only to members of the sheet owner's Smartsheet\norganization account.\n  * SHARED - available only to users shared to the item.\n\nOnly returned in the response if **readWriteEnabled = true**.\n",
                        "type": "string",
                        "enum": [
                            "ALL",
                            "ORG",
                            "SHARED"
                        ],
                        "path": "readWriteAccessibleBy"
                    },
                    "readWriteDefaultView": {
                        "description": "Indicates which view the user has set for a read-write, default view of the published sheet. Must be one of the listed enum values.\n",
                        "type": "string",
                        "enum": [
                            "CALENDAR",
                            "CARD",
                            "GRID"
                        ],
                        "path": "readWriteDefaultView"
                    },
                    "readWriteEnabled": {
                        "description": "If **true**,a rich version of the sheet is published with the ability to edit cells and manage attachments and discussions.",
                        "type": "boolean",
                        "path": "readWriteEnabled"
                    },
                    "readWriteShowToolbar": {
                        "description": "Indicates whether the left nav toolbar is displayed. The default, or **true**, is to display the toolbar. If **false**, hides the toolbar.",
                        "type": "boolean",
                        "deprecated": true,
                        "path": "readWriteShowToolbar"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "Content-Type": {
                        "type": "text",
                        "index": 0,
                        "label": "Content Type",
                        "tooltip": "<p>Required for POST and PUT requests. Defines the structure for the request body.</p>",
                        "defaultValue": "application/json"
                    },
                    "icalEnabled": {
                        "type": "toggle",
                        "index": 1,
                        "label": "Ical Enabled",
                        "tooltip": "<p>If <strong>true</strong>, a webcal is available for the calendar in the sheet.</p>"
                    },
                    "readOnlyFullAccessibleBy": {
                        "type": "select",
                        "index": 2,
                        "label": "Read Only Full Accessible By",
                        "tooltip": "<p>Indicates who can access the 'Read-Only Full' view of the published sheet:</p>\n<ul>\n<li>ALL - available to anyone who has the link.</li>\n<li>ORG - available only to members of the sheet owner's Smartsheet\norganization account.</li>\n<li>SHARED - available only to users shared to the item.</li>\n</ul>\n<p>Only returned in the response if <strong>readOnlyFullEnabled = true</strong>.</p>",
                        "options": [
                            {
                                "content": "ALL",
                                "value": "ALL"
                            },
                            {
                                "content": "ORG",
                                "value": "ORG"
                            },
                            {
                                "content": "SHARED",
                                "value": "SHARED"
                            }
                        ]
                    },
                    "readOnlyFullDefaultView": {
                        "type": "select",
                        "index": 3,
                        "label": "Read Only Full Default View",
                        "tooltip": "<p>Indicates which view the user has set for a read-only, default view of the published sheet. Must be one of the listed enum values.</p>",
                        "options": [
                            {
                                "content": "CALENDAR",
                                "value": "CALENDAR"
                            },
                            {
                                "content": "CARD",
                                "value": "CARD"
                            },
                            {
                                "content": "GRID",
                                "value": "GRID"
                            }
                        ]
                    },
                    "readOnlyFullEnabled": {
                        "type": "toggle",
                        "index": 4,
                        "label": "Read Only Full Enabled",
                        "tooltip": "<p>If <strong>true</strong>, a rich version of the sheet is published with the ability to download row attachments and discussions.</p>"
                    },
                    "readOnlyFullShowToolbar": {
                        "type": "toggle",
                        "index": 5,
                        "label": "Read Only Full Show Toolbar",
                        "tooltip": "<p>Indicates whether the left nav toolbar is displayed. The default, or <strong>true</strong>, is to display the toolbar. If <strong>false</strong>, hides the toolbar.</p>"
                    },
                    "readOnlyLiteEnabled": {
                        "type": "toggle",
                        "index": 6,
                        "label": "Read Only Lite Enabled",
                        "tooltip": "<p>If <strong>true</strong>, a lightweight version of the sheet is published without row attachments and discussions.</p>"
                    },
                    "readWriteAccessibleBy": {
                        "type": "select",
                        "index": 7,
                        "label": "Read Write Accessible By",
                        "tooltip": "<p>Indicates who can access the 'Edit by Anyone' view of the published sheet:</p>\n<ul>\n<li>ALL - available to anyone who has the link.</li>\n<li>ORG - available only to members of the sheet owner's Smartsheet\norganization account.</li>\n<li>SHARED - available only to users shared to the item.</li>\n</ul>\n<p>Only returned in the response if <strong>readWriteEnabled = true</strong>.</p>",
                        "options": [
                            {
                                "content": "ALL",
                                "value": "ALL"
                            },
                            {
                                "content": "ORG",
                                "value": "ORG"
                            },
                            {
                                "content": "SHARED",
                                "value": "SHARED"
                            }
                        ]
                    },
                    "readWriteDefaultView": {
                        "type": "select",
                        "index": 8,
                        "label": "Read Write Default View",
                        "tooltip": "<p>Indicates which view the user has set for a read-write, default view of the published sheet. Must be one of the listed enum values.</p>",
                        "options": [
                            {
                                "content": "CALENDAR",
                                "value": "CALENDAR"
                            },
                            {
                                "content": "CARD",
                                "value": "CARD"
                            },
                            {
                                "content": "GRID",
                                "value": "GRID"
                            }
                        ]
                    },
                    "readWriteEnabled": {
                        "type": "toggle",
                        "index": 9,
                        "label": "Read Write Enabled",
                        "tooltip": "<p>If <strong>true</strong>,a rich version of the sheet is published with the ability to edit cells and manage attachments and discussions.</p>"
                    },
                    "readWriteShowToolbar": {
                        "type": "toggle",
                        "index": 10,
                        "label": "Read Write Show Toolbar",
                        "tooltip": "<p>Indicates whether the left nav toolbar is displayed. The default, or <strong>true</strong>, is to display the toolbar. If <strong>false</strong>, hides the toolbar.</p>"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Message",
                    "value": "message"
                },
                {
                    "label": "Result Code",
                    "value": "resultCode"
                },
                {
                    "label": "Result",
                    "value": "result"
                },
                {
                    "label": "Result Ical Enabled",
                    "value": "result.icalEnabled"
                },
                {
                    "label": "Result Ical Url",
                    "value": "result.icalUrl"
                },
                {
                    "label": "Result Read Only Full Accessible By",
                    "value": "result.readOnlyFullAccessibleBy"
                },
                {
                    "label": "Result Read Only Full Default View",
                    "value": "result.readOnlyFullDefaultView"
                },
                {
                    "label": "Result Read Only Full Enabled",
                    "value": "result.readOnlyFullEnabled"
                },
                {
                    "label": "Result Read Only Full Show Toolbar",
                    "value": "result.readOnlyFullShowToolbar"
                },
                {
                    "label": "Result Read Only Full Url",
                    "value": "result.readOnlyFullUrl"
                },
                {
                    "label": "Result Read Only Lite Enabled",
                    "value": "result.readOnlyLiteEnabled"
                },
                {
                    "label": "Result Read Only Lite Ssl Url",
                    "value": "result.readOnlyLiteSslUrl"
                },
                {
                    "label": "Result Read Only Lite Url",
                    "value": "result.readOnlyLiteUrl"
                },
                {
                    "label": "Result Read Write Accessible By",
                    "value": "result.readWriteAccessibleBy"
                },
                {
                    "label": "Result Read Write Default View",
                    "value": "result.readWriteDefaultView"
                },
                {
                    "label": "Result Read Write Enabled",
                    "value": "result.readWriteEnabled"
                },
                {
                    "label": "Result Read Write Show Toolbar",
                    "value": "result.readWriteShowToolbar"
                },
                {
                    "label": "Result Read Write Url",
                    "value": "result.readWriteUrl"
                }
            ]
        }
    ],
    "properties": {},
    "auth": {
        "service": "appmixer:smartsheetapireference"
    },
    "icon": "data:image/png;base64,"
}