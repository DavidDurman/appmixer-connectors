{
    "name": "appmixer.facebookbusiness.marketing.GetAdAccounts",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Return a list of ad accounts associated with the user.",
    "auth": {
        "service": "appmixer:facebookbusiness",
        "scope": [
            "ads_management",
            "ads_read"
        ]
    },
    "quota": {
        "manager": "appmixer:facebookbusiness",
        "resources": "requests",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in"
        }
    ],
    "outPorts": [
        {
        "name": "out",
        "options": [
            {
            "value": "accounts",
            "label": "Accounts",
            "schema": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string",
                            "title": "ID (act_123)"
                        },
                        "account_id": {
                            "type": "string",
                            "title": "Account ID (123)"
                        },
                        "name": {
                            "type": "string",
                            "title": "Name"
                        },
                        "account_status": {
                            "type": "integer",
                            "title": "Status"
                        }
                    }
                }
            }
        }
        ]
    }
    ],
    "icon": "data:image/svg+xml;base64,"
}
