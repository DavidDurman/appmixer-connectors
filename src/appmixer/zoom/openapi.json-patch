[
    { "op": "add", "path": "/info/x-connector-service", "value": "zoom" },
    { "op": "add", "path": "/info/x-connector-module", "value": "meeting" },
    { "op": "add", "path": "/info/x-connector-icon", "value": "https://seeklogo.com/images/Z/zoom-fondo-azul-vertical-logo-8246E36E95-seeklogo.com.png" },
    { "op": "add", "path": "/components/securitySchemes/OAuth/x-connector-connection-check", "value": {
        "method": "GET",
        "url": "/users/me",
        "headers": {
            "Authorization": "Bearer {accessToken}"
        }
    } },
    { "op": "move", "from": "/paths/~1users~1{userId}~1meetings/post/parameters/0/schema/oneOf", "path": "/paths/~1users~1{userId}~1meetings/post/parameters/0/schema/anyOf" },
    { "op": "move", "from": "/paths/~1users~1{userId}~1meetings/get/parameters/0/schema/oneOf", "path": "/paths/~1users~1{userId}~1meetings/get/parameters/0/schema/anyOf" },
    { "op": "add", "path": "/components/securitySchemes/OAuth/x-connector-connection-profile", "value": {
        "method": "GET",
        "url": "/users/me",
        "headers": {
            "Authorization": "Bearer {accessToken}"
        },
        "transform": "email"
    } },
    { "op": "add", "jsonpath": "$.paths.*[?(@.operationId == 'meetings')]", "path": "x-connector-pagination", "value": {
        "type": "cursor",
        "parameters": {
            "limit": "page_size",
            "page": 20,
            "cursor": "next_page_token",
            "cursorPath": "next_page_token",
            "results": "meetings"
        }
    } },
    { "op": "add", "path": "/components/x-connector-webhooks", "value": {
        "events": {
            "type": "static",
            "path": "/events",
            "pattern": "payload.event & ':' & payload.payload.account_id",
            "crc": {
                "condition": "payload.event = \"endpoint.url_validation\"",
                "alg": "sha256",
                "key": "webhookSecretToken",
                "challenge": "payload.payload.plainToken",
                "response": "{ \"encryptedToken\": responseToken, \"plainToken\": challenge }",
                "digest": "hex"
            }
        }
    } },
    { "op": "add", "path": "/webhooks", "value": {} },

    { "op": "add", "path": "/webhooks/meeting.created", "value": {
        "$ref": "./zoom/ZoomAPIEvents-MeetingSpec.json#/webhooks/meeting.created"
    } },
    { "op": "add", "path": "/webhooks/meeting.created/post/x-connector-webhook", "value": {
        "$ref": "#/components/x-connector-webhooks/events"
    } },
    { "op": "add", "path": "/webhooks/meeting.created/post/x-connector-webhook/topic", "value": "meeting.created:{$connection.profile#/account_id}" },

    { "op": "add", "path": "/webhooks/meeting.deleted", "value": {
