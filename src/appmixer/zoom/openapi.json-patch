    } },
    { "op": "add", "path": "/webhooks/meeting.deleted/post/x-connector-webhook", "value": {
        "$ref": "#/components/x-connector-webhooks/events"
    } },
    { "op": "add", "path": "/webhooks/meeting.deleted/post/x-connector-webhook/topic", "value": "meeting.deleted:{$connection.profile.account_id}" },

    { "op": "add", "path": "/webhooks/meeting.updated", "value": {
